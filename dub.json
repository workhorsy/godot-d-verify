{
	"name": "godot-d-verify",
	"description": "Verify a D Godot project",
	"authors": [
		"Matthew Brennan Jones <matthew.brennan.jones@gmail.com>"
	],
	"homepage": "https://github.com/workhorsy/godot-d-verify",
	"copyright": "Copyright (c) 2021 Matthew Brennan Jones",
	"license": "MIT",

	"configurations": [
		{
			"name": "default",
			"targetPath": "bin",
			"targetType": "executable",
			"targetName": "godot-d-verify",
			"importPaths": ["source/"],
			"dependencies": {
				"dscanner": "~>0.11.0",
				"dxml": "~>0.4.3"
			}
		},
		{
			"name": "unittest",
			"targetPath": "lib",
			"targetType": "staticLibrary",
			"targetName": "godot-d-verify-test",
			"importPaths": ["source/", "test/"],
			"sourceFiles": ["test/test.d"],
			"excludedSourceFiles": ["source/main.d"],
			"dependencies": {
				"bdd": "~>3.0.0",
				"dscanner": "~>0.11.0",
				"dxml": "~>0.4.3"
			}
		}
	]
}
