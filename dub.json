{
	"name": "godot-d-verify",
	"description": "Verify a D Godot project",
	"authors": [
		"Matthew Brennan Jones <matthew.brennan.jones@gmail.com>"
	],
	"homepage": "https://github.com/workhorsy/godot-d-verify",
	"copyright": "Copyright (c) 2021 Matthew Brennan Jones",
	"license": "MIT",
	"importPaths": ["source/"],

	"configurations": [
		{
			"name": "default",
			"targetPath": ".",
			"targetType": "executable",
			"targetName": "godot-d-verify",
			"dependencies": {
				"dxml": { "path": "../third_party/dxml-0.4.3" },
				"libdparse": { "path": "../third_party/libdparse-0.17.0" },
				"stdx-allocator": { "path": "../third_party/stdx-allocator-2.77.5" },
			}
		},
		{
			"name": "unittest",
			"targetPath": "lib",
			"targetType": "staticLibrary",
			"targetName": "godot-d-verify-test",
			"excludedSourceFiles": ["source/main.d"],
			"dependencies": {
				"bdd": { "path": "../third_party/BDD-3.1.0" },
				"dxml": { "path": "../third_party/dxml-0.4.3" },
				"libdparse": { "path": "../third_party/libdparse-0.17.0" },
				"stdx-allocator": { "path": "../third_party/stdx-allocator-2.77.5" },
			}
		}
	]
}
